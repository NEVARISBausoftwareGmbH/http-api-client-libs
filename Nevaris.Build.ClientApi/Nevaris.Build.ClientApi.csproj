<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>latest</LangVersion>
    <PackageProjectUrl>https://github.com/NEVARISBausoftwareGmbH/http-api-client-libs</PackageProjectUrl>
    <RepositoryUrl>https://github.com/NEVARISBausoftwareGmbH/http-api-client-libs</RepositoryUrl>
    <PackageTags>Nevaris</PackageTags>
    <Description>A client library for accessing Nevaris Build via its RESTful API</Description>
    <Authors>Nevaris Bausoftware GmbH</Authors>
    <Company>Nevaris Bausoftware GmbH</Company>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Version>4.0.0</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Title>Nevaris.Build.ClientApi</Title>
    <Copyright>NEVARIS Bausoftware GmbH</Copyright>
    <PackageLicenseUrl></PackageLicenseUrl>
    <PackageReleaseNotes>4.2.0: ClientApi für Build 2023.0: Unterstützung für Individualeigenschaften (Custom Properties) + weitere LV-Eigenschaften
4.2.2: Fix für UpdateLeistungsverzeichnis (war irrtümlich als Post-Operation markiert)</PackageReleaseNotes>
    <PackageVersion>4.2.2</PackageVersion>
    <AssemblyVersion>4.2.2</AssemblyVersion>
    <FileVersion>4.2.2</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Refit" Version="6.3.2" />
    <PackageReference Include="Refit.Newtonsoft.Json" Version="6.3.2" />
  </ItemGroup>

</Project>
